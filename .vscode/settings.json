{
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django",
        "--django-settings-module=levelup.settings"
    ],
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "name": "Level Up",
            "database": "db.sqlite3"
        }
    ],
    "cSpell.words": [
        "asgi",
        "gametype",
        "mzdgqh",
        "reauthenticate",
        "tpxp",
        "vcqkhyv"
    ],
    "cSpell.ignoreWords": [
        "arneson",
        "datetime",
        "gielinor",
        "gygax",
        "jagex",
        "mmorpg",
        "wargaming"
    ]
}
